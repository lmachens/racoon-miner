    "prettier": "1.12.1",
    "regenerator-runtime": "^0.11.1",
    "rollup": "^0.58.2",
    "rollup-plugin-babel": "^4.0.0-beta.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0",
    "rollup-watch": "^4.3.1",
    "standard-version": "^4.3.0"
  },
  "scripts": {
    "dev": "rollup -c -w",
    "production": "rollup -c production.rollup.config.js",
    "test": "jest",
    "release": "standard-version",
    "github-release": "set CONVENTIONAL_GITHUB_RELEASER_TOKEN=059ff991289433e238681d73c00f04b89c8f1627 && conventional-github-releaser -p angular",
    "precommit": "lint-staged",
    "prepush": "jest",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "standard-version": {
    "scripts": {
      "postcommit": "node tools/syncVersion && git commit -am \"chore(overwolf): manifest version bump\" && git push --follow-tags origin master && npm run github-release"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,css}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lmachens/raccoon-miner.git"
  },
  "keywords": [
    "overwolf",
    "crypto",
    "mining",
    "cryptocurrency",
    "ethereum"
  ],
  "author": "Leon Machens",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lmachens/raccoon-miner/issues"
  },
  "homepage": "https://github.com/lmachens/raccoon-miner"
