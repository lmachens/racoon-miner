  "version": "0.6.1",
  "description": "Crypto mining app for Overwolf appstore",
  "dependencies": {
    "@material-ui/core": "^1.0.0-rc.1",
    "@material-ui/icons": "^1.0.0-rc.0",
    "classnames": "^2.2.5",
    "lodash": "^4.17.10",
    "material-ui": "^1.0.0-beta.44",
    "prop-types": "^15.6.1",
    "prune": "0.0.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-ga": "^2.5.2",
    "react-redux": "^5.0.7",
