{
  "manifest_version": "1",
  "type": "WebApp",
  "meta": {
    "name": "Raccoon Miner",
    "version": "0.1.0",
    "minimum-overwolf-version": "0.85.0.0",
    "author": "Leon Machens",
    "icon": "assets/icon.png",
    "icon_gray": "assets/icon_gray.png",
    "description": "Crypto mining app for Overwolf appstore"
  },
  "externally_connectable": {
    "matches": ["http://localhost:35729"]
  },
  "permissions": [],
  "data": {
    "start_window": "main",
    "windows": {
      "main": {
        "file": "main.html",
        "transparent": false,
        "resizable": true,
        "show_in_taskbar": true,
        "size": {
          "width": 400,
          "height": 400
        },
        "start_position": {
          "Top": 100,
          "Left": 300
        }
      }
    },
    "extra-objects": {
      "process-manager-plugin": {
        "file": "plugins/process_manager.dll",
        "class": "com.overwolf.net.ProcessManager"
      },
      "simple-io-plugin": {
        "file": "plugins/simple-io-plugin.dll",
        "class": "overwolf.plugins.SimpleIOPlugin"
      }
    }
  }
}
